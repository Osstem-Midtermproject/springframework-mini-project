<?xml version="1.0" encoding="UTF-8" ?>

<!-- 문서 유형 선언 -->
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd"> <!-- 여기에 정의되어 있는 태그를 사용하겠다 -->

<!-- 루트 태그(엘리먼트) -->
<configuration>

	<typeAliases>
		<!-- pager -->
		<typeAlias type="com.mycompany.webapp.dto.Pager" alias="pager" />
	
		<!-- 요청내역 -->
		<typeAlias type="com.mycompany.webapp.dto.RequestDetails" alias="requestDetails"/>		
		<!-- 진행상황 -->
		<typeAlias type="com.mycompany.webapp.dto.Progress" alias="progress"/>
		<!-- 시공스케줄-->
		<typeAlias type="com.mycompany.webapp.dto.ConstructionSchedule" alias="constructionSchedule"/>
		<!-- 병원 -->
		<typeAlias type="com.mycompany.webapp.dto.Hospital" alias="hospital"/>
		<!-- 팀 -->
		<typeAlias type="com.mycompany.webapp.dto.Team" alias="team"/>	
		<!-- 계약 -->
		<typeAlias type="com.mycompany.webapp.dto.Contract" alias="contract"/>									
		<!-- user -->
		<typeAlias type="com.mycompany.webapp.dto.Users" alias="user"/>	
		<!-- 진행상태 -->
		<typeAlias type="com.mycompany.webapp.dto.State" alias="state"/>
		<!-- 추가요청 -->
		<typeAlias type="com.mycompany.webapp.dto.AdditionalRequest" alias="additionalRequest"/>
		<!-- 팀히스토리 -->
		<typeAlias type="com.mycompany.webapp.dto.TeamHistory" alias="teamHistory"/>
		<!-- 병원카테고리 -->
		<!-- HosptialAndCategory : 체크박스로 선택된 항목에 해당하는 리스트 뽑기 위해-->
		<typeAlias type="com.mycompany.webapp.dto.HosptialAndCategory" alias="hac"/>
		<!-- ThtidAndStartDateAndEndDate : 팀과 분기별로 매출 총액을 구하기 위해-->
		<typeAlias type="com.mycompany.webapp.dto.ThtidAndStartDateAndEndDate" alias="tsd"/>
		<!-- 디자인 -->
		<typeAlias type="com.mycompany.webapp.dto.Design" alias="design"/>
		<!-- 상담 -->
		<typeAlias type="com.mycompany.webapp.dto.CounselingSchedule" alias="counselingSchedule"/>
		<!-- AS 스케줄 -->
		<typeAlias type="com.mycompany.webapp.dto.AsSchedule" alias="asSchedule"/>
		
		
	</typeAliases>
	
</configuration>